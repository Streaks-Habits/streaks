{
	"name": "streaks",
	"version": "0.0.1",
	"description": "",
	"main": "dist/server.js",
	"scripts": {
		"start": "node dist/scripts/webserv/server.js",
		"start:dev": "nodemon --watch src --watch .env -e env,json,ts,ejs,scss,js --exec 'npm run build && npm run start'",
		"build": "npm run build:clear && tsc && npm run build:static && npm run build:sass",
		"build:static": "mkdir -p dist/public && cp -r src/views dist/views && cp -r src/public/* dist/public",
		"build:sass": "sass src/styles/:dist/public/styles/",
		"build:clear": "rm -rf dist",
		"test": "npm run test:lint",
		"test:lint": "eslint src/ user_scripts/"
	},
	"author": "cestoliv",
	"license": "MIT",
	"dependencies": {
		"bcrypt": "^5.0.1",
		"chalk": "v4.1.2",
		"cookie-parser": "^1.4.6",
		"dotenv": "^16.0.1",
		"ejs": "^3.1.8",
		"envfile": "^6.17.0",
		"express": "^4.18.1",
		"express-session": "^1.17.3",
		"jsonwebtoken": "^8.5.1",
		"matrix-js-sdk": "^19.0.0",
		"moment": "^2.29.4",
		"mongoose": "^6.4.6",
		"node-localstorage": "^2.2.1",
		"node-schedule": "^2.1.0",
		"olm": "https://packages.matrix.org/npm/olm/olm-3.1.4.tgz",
		"readline-sync": "^1.4.10",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cookie-parser": "^1.4.3",
		"@types/express": "^4.17.13",
		"@types/express-session": "^1.17.5",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/node": "^18.0.6",
		"@types/node-localstorage": "^1.3.0",
		"@types/node-schedule": "^2.1.0",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.30.7",
		"@typescript-eslint/parser": "^5.30.7",
		"eslint": "^8.20.0",
		"eslint-plugin-jsdoc": "^39.3.3",
		"sass": "^1.5.1",
		"typescript": "^4.7.4"
	}
}
